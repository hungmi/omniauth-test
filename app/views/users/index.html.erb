<div class="container">
  <div class="row">
    <h3>Users</h3>
    <div class="column">
      <table class="table">
        <tbody>
          <% @users.each do |user| %>
            <tr>
              <%= render user %>
            </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>
<div class="fb-comments" data-href="http://localhost:5678/users" data-width="300" data-numposts="10" data-order-by="social" data-colorscheme="light"></div>
